{
     
     "builds": [{
           "src": "hosteldb/wsgi.py",
           "use": "@vercel/python",
           "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}


        }], 
     "routes": [
     
            {
                "src": "/(.*)",
                "dest": "hosteldb/wsgi.py",
               
            }

      ]
}